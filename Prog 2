#include <stdio.h>
int sum(int a,int b);
int main()
{
    int a,b,addition;
    printf("\nEnter the two integers to be added\n");
    scanf("%d%d",&a,&b);
    addition=sum(a,b);
    printf("\nThe addition of two integers is : %d\n",addition);
    return 0;
}
int sum(int a,int b)
{
    int addition;
    addition=a+b;
    return addition;
}
